{
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "typescript-eslint-parser"
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended",
        "prettier"
    ],
    "plugins": [
        "typescript",
        "prettier"
    ],
    "rules": {
        "no-undef": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "semi": false
            }
        ]
    }
}